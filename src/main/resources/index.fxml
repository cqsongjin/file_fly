            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </GridPane.margin>
        </Label>
      <ProgressBar fx:id="process_line_bar" maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.71" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
      <HBox fx:id="file_box" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #b6b6b6;" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="2">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="拖拽文件到此处" />
